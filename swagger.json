{
   "swagger":"2.0",
   "info":{
      "version":"1.0.0",
      "description":"Taxi Booking Management System",
      "title":"Taxi Booking"
   },
   "host":"127.0.0.1:8081",
   "basePath":"/",
   "tags": [
   {
      "name": "Driver",
      "description": "API for Driver in the Project"
   },
   {
      "name": "Manager",
      "description": "API for Manager in the system" 
   },
   {
      "name": "User",
      "description": "API for User in the system"
   }],
   "schemes":[
      "http","https"
   ],
   "paths":{
      "/driver/register":{
         "post":{
            "description":"Driver Registeration API",
             "tags":["Driver"],
            "parameters":[
               {
                  "type":"string",
                  "description":"Name of the Driver",
                  "name":"name",
                  "in":"formData",
                  "example":"Shubham Negi"
               },
               {
                  "type":"string",
                  "description":"Password for the Driver",
                  "name":"pass",
                  "in":"formData",
                  "example":"s123"
               },
               {
                  "type":"email",
                  "description":"Email of the Driver",
                  "name":"email",
                  "in":"formData",
                  "example":"shubham@gmail.com"
               },
               {
                  "type":"string",
                  "description":"Car Name",
                  "name":"car_name",
                  "in":"formData",
                  "example":"Tata Indigo"
               },
               {
                  "type":"string",
                  "description":"Car Name",
                  "name":"car_number",
                  "in":"formData",
                  "example":"UK 07 P 1234"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Register"
               }
            }
         }
      },
      "/driver/login":{
         "post":{
            "description":"Driver Login API",
            "tags":["Driver"],
            "parameters":[
               {
                  "type":"string",
                  "description":"Email of the Driver",
                  "name":"email",
                  "in":"formData",
                  "example":"shubham@gmail.com"
               },
               {
                  "type":"string",
                  "description":"Password of the Driver",
                  "name":"pass",
                  "in":"formData",
                  "example":"s123"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Login"
               }
            }
         }
      },
      "/driver/booking":{
         "get":{
            "description":"Driver Show Booking API",
            "tags":["Driver"],
            "responses":{
               "200":{
                  "description":"Show Current Bookings"
               }
            }
         }
      },
      "/driver/complete":{
         "delete":{
            "description":"Driver Complete Booking API",
            "tags":["Driver"],
            "responses":{
               "200":{
                  "description":"Complete Current Bookings"
               }
            }
         }
      },
      "/driver/logout":{
         "delete":{
            "description":"Driver Logout API",
            "tags":["Driver"],
            "responses":{
               "200":{
                  "description":"Success Logout"
               }
            }
         }
      },
      "/user/register":{
         "post":{
            "description":"User Register API",
            "tags":["User"],
            "parameters":[
               {
                  "type":"string",
                  "description":"Email of the User",
                  "name":"email",
                  "in":"formData",
                  "example":"manik@gmail.com"
               },
               {
                  "type":"string",
                  "description":"Name of the User",
                  "name":"name",
                  "in":"formData",
                  "example":"Manik Rastogi"
               },
               {
                  "type":"string",
                  "description":"Password for the User",
                  "name":"pass",
                  "in":"formData",
                  "example":"m123"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Register"
               }
            }
         }
      },
      "/user/login":{
         "post":{
            "description":"User Login API",
            "tags":["User"],
            "parameters":[
               {
                  "type":"string",
                  "description":"Email of the User",
                  "name":"email",
                  "in":"formData",
                  "example":"manik@gmail.com"
               },
               {
                  "type":"string",
                  "description":"Password for the User",
                  "name":"pass",
                  "in":"formData",
                  "example":"m123"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Login"
               }
            }
         }
      },
      "/user/createBooking":{
         "post":{
            "description":"User Login API",
            "tags":["User"],
            "parameters":[
               {
                  "type":"number",
                  "description":"From Location Latitude",
                  "name":"from_lat",
                  "in":"formData",
                  "example":"12.1234"
               },
               {
                  "type":"number",
                  "description":"From Location Longitude",
                  "name":"from_long",
                  "in":"formData",
                  "example":"12.1234"
               },
               {
                  "type":"number",
                  "description":"To Location Latitude",
                  "name":"to_lat",
                  "in":"formData",
                  "example":"12.1234"
               },
               {
                  "type":"number",
                  "description":"To Location Longitude",
                  "name":"to_long",
                  "in":"formData",
                  "example":"12.1234"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Create Booking"
               }
            }
         }
      },
      "/user/booking":{
         "get":{
            "description":"Return Users Bookings API",
            "tags":["User"],
            "responses":{
               "200":{
                  "description":"User's Booking"
               }
            }
         }
      },
      "/user/booking/{bookingId}":{
         "get":{
            "description":"Return Users Bookings API",
            "tags":["User"],
            "responses":{
               "200":{
                  "description":"User's Booking"
               }
            }
         }
      },
      "/user/bookingHistory":{
         "get":{
            "description":"Return Users Bookings API",
            "tags":["User"],
            "responses":{
               "200":{
                  "description":"User's Booking History"
               }
            }
         }
      },
      "/manager/login":{
         "post":{
            "description":"User Login API",
            "tags":["Manager"],
            "parameters":[
               {
                  "type":"string",
                  "description":"Email of the Manager",
                  "name":"email",
                  "in":"formData",
                  "example":"akash@gmail.com"
               },
               {
                  "type":"string",
                  "description":"Password for the Manager",
                  "name":"pass",
                  "in":"formData",
                  "example":"a123"
               }
            ],
            "responses":{
               "200":{
                  "description":"Success Login"
               }
            }
         }
      },
      "/manager/process":{
         "get":{
            "description":"Booking in Process API",
            "tags":["Manager"],
            "responses":{
               "200":{
                  "description":"Current Process Booking"
               }
            }
         }
      },
      "/manager/driver":{
         "get":{
            "description":"All Drivers API",
            "tags":["Manager"],
            "responses":{
               "200":{
                  "description":"All Drivers"
               }
            }
         }
      },
      "/manager/liveDriver":{
         "get":{
            "description":"Live Drivers API",
            "tags":["Manager"],
            "responses":{
               "200":{
                  "description":"All Live Drivers"
               }
            }
         }
      },
      "/manager/pastBooking":{
         "get":{
            "description":"All Past Booking API",
            "tags":["Manager"],
            "responses":{
               "200":{
                  "description":"All Past Booking"
               }
            }
         }
      }
   }
}